{
    "name": "leoparden/minify",
    "description": "Pimcore Minify HTML, Stylesheet and Javascript",
    "keywords": ["pimcore", "minify"],
    "type": "pimcore-bundle",
    "require": {
        "php": ">=7.1",
        "pimcore/pimcore": ">=5.5.4",
        "leafo/lessphp": "^0.5.0",
        "voku/html-min": "^3.0",
        "matthiasmullie/minify": "^1.3"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Leoparden e.K.",
            "email": "technik@die-leoparden.de"
        }
    ],
    "autoload": {
        "psr-4": {
            "MinifyBundle\\": "src/MinifyBundle"
        }
    },
    "extra": {
        "pimcore": {
            "bundles": [
                "MinifyBundle\\MinifyBundle"
            ]
        }
    },
    "minimum-stability": "dev"
}
